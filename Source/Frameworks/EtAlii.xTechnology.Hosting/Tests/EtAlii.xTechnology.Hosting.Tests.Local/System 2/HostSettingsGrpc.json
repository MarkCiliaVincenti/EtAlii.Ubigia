{
    "Host": {
        "Diagnostics": {
            "InjectProfiling": false,
            "InjectLogging": true,
            "InjectDebugging": false
        },

        "TestHost": "{{HOST:AuthenticationHost@127.0.0.1}}",
        "TestPort": "{{PORT:AuthenticationPort@5003}}",
        "TestHostFolder": "{{FOLDER:AuthenticationFolder@%localappdata%\\EtAlii\\App}}",

    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },

    "Storage": {
        "Factory": "EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc.StorageServiceFactory, EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc",
        "Name": "Debug storage",
        "BaseFolder": "{{FOLDER:StorageFolder@%LOCALAPPDATA%\\EtAlii\\Ubigia}}",
        "Diagnostics": {
            "InjectProfiling": false,
            "InjectLogging": true,
            "InjectDebugging": false
        }
    },
    "Infrastructure": {
        "Factory": "EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc.InfrastructureServiceFactory, EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc",
        "Name": "Debug storage"
    },

    "Authentication": {
        "Factory": "EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc.AuthenticationServiceFactory, EtAlii.xTechnology.Hosting.Tests.Infrastructure.Grpc"
    },
    "Management-Api-Grpc": {
        "Comment": "// Admin should be accessible over localhost only",
        "Factory": "EtAlii.xTechnology.Hosting.Tests.Infrastructure.Admin.Api.Grpc.AdminServiceFactory, EtAlii.xTechnology.Hosting.Tests.Infrastructure.Admin.Api.Grpc",
        "IpAddress": "127.0.0.1",
        "Port": "{{PORT:AdminApiPortGrpc@5000}}",
        "Path": "{{PATH:AdminApiPathGrpc@}}"
    },
    "User-Api-Grpc": {
        "Factory": "EtAlii.xTechnology.Hosting.Tests.Infrastructure.User.Api.Grpc.UserServiceFactory, EtAlii.xTechnology.Hosting.Tests.Infrastructure.User.Api.Grpc",
        "IpAddress": "0.0.0.0",
        "Port": "{{PORT:UserApiPortGrpc@5001}}",
        "Path": "{{PATH:UserApiPathGrpc@}}"
    }
}
