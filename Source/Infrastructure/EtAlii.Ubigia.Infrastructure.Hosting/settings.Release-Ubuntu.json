{
    "@jdt.merge" : [
        {
            "@jdt.path": "$.Storage",
            "@jdt.value": {
                "Factory": "EtAlii.Ubigia.Infrastructure.Fabric.Standard.StorageServiceFactory, EtAlii.Ubigia.Infrastructure.Fabric.Standard",
                "BaseFolder": "%UBIGIA_DATA_FOLDER%"
            }
        },
        {
            "@jdt.path": "$.Host",
            "@jdt.value": {
                "CertificateFile": "%UBIGIA_CERTIFICATE_FILE%",
                "CertificatePassword": "%UBIGIA_CERTIFICATE_PASSWORD%"
            }
        },
        {
            "@jdt.path": "$.Management-Api-Grpc",
            "@jdt.value": {
                "Port": "%UBIGIA_MANAGEMENT_API_PORT%",
                "IpAddress": "%UBIGIA_MANAGEMENT_API_INTERFACE%"
            }
        },
        {
            "@jdt.path": "$.Management-Portal",
            "@jdt.value": {
                "Port": "%UBIGIA_MANAGEMENT_PORTAL_PORT%",
                "IpAddress": "%UBIGIA_MANAGEMENT_PORTAL_INTERFACE%"
            }
        },
        {
            "@jdt.path": "$.User-Api-Grpc",
            "@jdt.value": {
                "Port": "%UBIGIA_USER_API_PORT%",
                "IpAddress": "%UBIGIA_USER_API_INTERFACE%"
            }
        },
        {
            "@jdt.path": "$.Serilog.WriteTo[0].Args.configure[1].Args.serverUrl",
            "@jdt.value": "%SERILOG_SERVER_URL%"
        },
        {
            "@jdt.path": "$.Serilog.WriteTo[0].Args.configure[0].Args.restrictedToMinimumLevel",
            "@jdt.value": "Information"
        }]
}
